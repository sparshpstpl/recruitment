[ignore]
.*/node_modules/.*\(test\).*\.json$

.*/crm/.*
.*/web/.*
.*/packages/.*

.*/node_modules/jsonlint-lines-primitives/.*
.*/node_modules/grpc/.*

.*/build/.*
.*/coverage/.*
.*/migrations/.*

[untyped]
.*/src/utils/.*
.*/src/errors.js
.*/src/app.js
.*/src/storage.js
.*/src/server.js
.*/src/Validator.js
.*/node_modules/dataloader/*
.*/node_modules/graphql-relay/lib/connection/*
.*/node_modules/graphql/*


[include]
../node_modules

[libs]
../flow-typed

[lints]
sketchy-null=error
# unsafe-getters-setters=error
# untyped-type-import=error # enable as soon as possible

[options]
esproposal.export_star_as=enable
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=../node_modules

include_warnings=true
